<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/changingItem_layout_1"
    android:layout_width="170dp"
    android:layout_height="90dp"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="1dp">

    <LinearLayout
        android:id="@+id/changingItem_layout_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/changingItem_textView_1"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="postponed"
            android:textSize="24dp" />

        <TextView
            android:id="@+id/changingItem_textView_2"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="not done"
            android:textSize="24dp" />

        <TextView
            android:id="@+id/changingItem_textView_3"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="done"
            android:textSize="24dp" />
    </LinearLayout>
</FrameLayout>
